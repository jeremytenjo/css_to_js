{
  "name": "css-to-js",
  "displayName": "CSS to JS",
  "description": "Convert css to javascript object",
  "version": "0.0.2",
  "publisher": "tgreen7",
  "engines": {
    "vscode": "^1.46.0"
  },
  "homepage": "https://github.com/tgreen7/css_to_js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tgreen7/css_to_js"
  },
  "bugs": {
    "url": "https://github.com/tgreen7/css_to_js/issues"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.convertCSStoJS"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.convertCSStoJS",
        "title": "Convert CSS-to-JS"
      }
    ],
    "keybindings": [
      {
        "command": "extension.convertCSStoJS",
        "key": "ctrl+shift+j",
        "mac": "cmd+shift+j",
        "when": "editorTextFocus"
      }
    ]
  },
  "keywords": [
    "css",
    "css_to_js",
    "keybindings",
    "javascript"
  ],
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --max-warnings=0",
      "prettier --write",
      "git add"
    ]
  }
}
